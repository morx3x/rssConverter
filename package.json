{
  "name": "rss-converter",
  "version": "1.0.9",
  "description": "This is a converter for rss to json.",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest --silent=false"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morx3x/rssConverter.git"
  },
  "keywords": [
    "rss",
    "json",
    "convertor",
    "rssConvertor"
  ],
  "author": "Yuki Morota <moro@studio.design> (https://twitter.com/morx3x)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morx3x/rssConverter/issues"
  },
  "homepage": "https://github.com/morx3x/rssConverter#readme",
  "dependencies": {
    "@types/xml2js": "^0.4.4",
    "axios": "^0.19.0",
    "xml2js": "^0.4.19"
  },
  "publishConfig": {
    "access": "public"
  }
}
